{
  "name": "material",
  "private": true,
  "version": "1.3.1",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && webpack --progress --bail --env dist -p",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "lint-sass": "sass-lint -c .sass-lint.yml 'src/styles/**/*.scss' -v -q",
    "posttest": "npm run lint",
    "reinstall": "rimraf node_modules && npm install",
    "serve:dev": "webpack-dev-server --open --env dev",
    "serve:server": "export NODE_ENV=development && webpack --colors --display-error-details --config ./server/webpack/webpack.config.js --env.server && node compiled/server.dev.js&",
    "serve:dist": "webpack-dev-server --open --env dist -p --progress",
    "start": "npm run serve:server && npm run serve:dev"
  },
